# Source the PDK xscherror_amplifier_N_input_v2rc file
if {![info exists PDK]} {
    source $env(PDK_ROOT)/$env(PDK)/libs.tech/xscherror_amplifier_N_input_v2/xscherror_amplifier_N_input_v2rc
}

# Add current directory to xscherror_amplifier_N_input_v2 library path
append XSCHEM_LIBRARY_PATH :[file dirname [info script]] 
